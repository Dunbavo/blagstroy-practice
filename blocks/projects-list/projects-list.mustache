{{#items.isNotEmpty}}
    <div class="projects-list">
        <div class="projects-list-items">
            {{#items}}
                <div class="projects-list-card{{^chars}} projects-list-card_not-chars{{/chars}}">
                    {{#image}}
                        <div class="projects-list-card-image-wrap">
                            <div class="projects-list-card-image">
                                <picture class="projects-list-card-picture">
                                    {{#mdSrc}}
                                        <source media="(max-width: 767px)" srcset="{{.}} 2x, {{.}}">
                                    {{/mdSrc}}
                                    <img src="{{src}}"
                                         alt="{{alt}}"
                                        {{#width}} width="{{.}}"{{/width}}
                                        {{#height}} height="{{.}}"{{/height}}
                                         class="projects-list-card__img"
                                         loading="lazy">
                                </picture>
                            </div>
                        </div>
                    {{/image}}
                    <div class="projects-list-card-content">
                            <div class="projects-list-card-content-top">
                                <div class="projects-list-card__name">{{{name}}}</div>
                                {{#description}}
                                    <div class="projects-list-card__description">{{{.}}}</div>
                                {{/description}}
                            </div>
                            <div class="projects-list-card-content-bottom">
                                <a href="{{{link}}}" class="projects-list-card__link">Подробнее
                                    <span class="projects-list-card-link__icon">
                                        <svg width="26" height="14" viewBox="0 0 26 14" fill="none" xmlns="http://www.w3.org/2000/svg">
                                            <path d="M24.8902 6.73488L19.2652 1.10988C19.1187 0.963379 18.8814 0.963379 18.735 1.10988C18.5885 1.25637 18.5885 1.49368 18.735 1.64014L23.7198 6.62501L1.5 6.62501C1.29272 6.62501 1.12499 6.79274 1.12499 7.00002C1.12499 7.2073 1.29272 7.37503 1.5 7.37503L23.7198 7.37504L18.735 12.3599C18.5885 12.5064 18.5885 12.7437 18.735 12.8901C18.8082 12.9634 18.9042 13 19.0001 13C19.0961 13 19.192 12.9634 19.2653 12.8901L24.8903 7.26514C25.0367 7.11868 25.0367 6.88137 24.8902 6.73488Z" fill="white" stroke="white"/>
                                        </svg>
                                    </span>
                                </a>
                                {{#date}}
                                    <div class="projects-list-card__release-date">
                                        {{#title}}
                                            <div class="projects-list-card-release-date__title">{{.}}</div>
                                        {{/title}}
                                        <div class="projects-list-card-release-date__text">{{text}}</div>
                                    </div>
                                {{/date}}
                            </div>
                            {{#chars.address}}
                                <div class="projects-list-card__address">{{.}}</div>
                            {{/chars.address}}
                            {{#chars}}
                                <div class="projects-list-card__chars">
                                    {{#type}}
                                        <div class="projects-list-card-chars-item">
                                            <div class="projects-list-card-chars__title">Тип</div>
                                            <div class="projects-list-card-chars__desc">{{{.}}}</div>
                                        </div>
                                    {{/type}}
                                    {{#price}}
                                        <div class="projects-list-card-chars-item">
                                            <div class="projects-list-card-chars__title">Цена</div>
                                            <div class="projects-list-card-chars__desc">{{{.}}}
                                                {{#squareMeter}} <span>{{{.}}}</span>{{/squareMeter}}
                                            </div>
                                        </div>
                                    {{/price}}
                                    {{#address}}
                                        <div class="projects-list-card-chars-item">
                                            <div class="projects-list-card-chars__title">Адрес</div>
                                            <div class="projects-list-card-chars__desc">{{{.}}}</div>
                                        </div>
                                    {{/address}}
                                </div>
                            {{/chars}}
                    </div>
                    <a href="{{detailPageUrl}}" class="link-as-card"></a>
                </div>
            {{/items}}
        </div>
        {{#linkAll}}
            <a href="{{.}}" class="projects-list__link">Все проекты</a>
        {{/linkAll}}
    </div>
{{/items.isNotEmpty}}