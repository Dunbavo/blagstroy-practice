    <form method="post" action="{{action}}" class="excursion-form form"{{#ajax_component_id}}
          id="{{.}}"{{/ajax_component_id}}>
        <div class="form-top">
            {{#title}}
                <div class="form__title">{{{.}}}</div>
            {{/title}}
            {{#sub_title}}
                <div class="form__subtitle">{{{.}}}</div>
            {{/sub_title}}
        </div>
        {{#peculiarities.isNotEmpty}}
            <div class="excursion-form-peculiarities peculiarities">
                {{#peculiarities}}
                    <div class="peculiarities-item{{#active}} peculiarities-item_active{{/active}}">
                        {{#number}}
                            <div class="peculiarities-item__number">
                                <span class="peculiarities-item__number-digit">{{.}}</span>
                                <span class="peculiarities-item__number-point">.</span>
                            </div>
                        {{/number}}
                        <div class="peculiarities-item__desc">{{{desc}}}</div>
                    </div>
                {{/peculiarities}}
            </div>
        {{/peculiarities.isNotEmpty}}
        <div class="form-content">
            <div class="form-content-fields">
                {{#html_fields}}
                    {{{html}}}
                {{/html_fields}}
            </div>
            <div class="form-bottom">
                <button class="form__submit button button_submit" name="submit" type="submit">
                    {{{submit_text}}}
                </button>
                {{#user_consent}}
                    <div class="form-group form-uc-wrapper">
                        <label class="form-label form-uc form-checkbox">
                            <input type="checkbox"
                                   id="USER_CONSENT_{{ajax_component_id}}" name="USER_CONSENT" value="Y" {{#checked}} checked{{/checked}}
                                   class="form-checkbox__control form-hidden form-uc__control">
                            <span class="form-checkbox__marker">
                                  <svg xmlns="http://www.w3.org/2000/svg" width="11" height="8" fill="none" viewBox="0 0 11 8"><path
                                    fill="#342733CC" fill-opacity="0" d="M9.68 0 4.266 5.379 1.32 2.453 0 3.763 4.265 8 11 1.31 9.68 0Z"/></svg>
                            </span>
                            <span class="form-checkbox__text">{{{.}}}</span>
                        </label>
                    </div>
                {{/user_consent}}
            </div>
        </div>
        <script>
            if (!window.initFeedback) window.initFeedback = []

            window.initFeedback.push(() => {
                new window.FeedbackForm('#{{ajax_component_id}}', {
                    options: {
                        showErrorMessages: true
                    }
                })
            })
        </script>
    </form>
</div>
